<%= form_for @user do |f| %>
  <div>
    <%=f.label :name %>
    <%= f.text_field :name%>
  </div>

  <div>
    <%= f.label :location %>
    <%= f.text_field :location %>
  </div>

  <div>
    <%= f.label :sex %>
    <%= f.text_field :sex %>
  </div>

  <div>
    <%= f.label :phone_number %>
    <%= f.text_field :phone_number%>
  </div>

  <div>
    <%= f.label :email %>
    <%= f.text_field :email %>
  </div>

  <div>
    <%= f.label :fblink %>
    <%= f.text_field :fblink %>
  </div>

   <div>
    <%= f.label :sclink %>
    <%= f.text_field :sclink %>
  </div>

  <div>
    <%= f.label :twlink%>
    <%= f.text_field :twlink %>
  </div>

  <%= f.submit %>
<% end %>


<%= link_to "Back to the listings", userlistings_path%>
