<!-- This is the index page of the site -->

<h1>Userlistings#index</h1>
<p>Find me in app/views/userlistings/index.html.erb</p>


<% if session[:user_id] %>

  <a href="/userlistings/new">Post to a board</a>
      <br>
    <a href="/logout"> Logout </a>
      <br>
    <%= link_to 'Edit your profile', edit_user_path(User.find(session[:user_id])) %>



  <% else %>
    <a href="/login">Login</a>
  <% end %>
    <br>
    <a href="/users/new">Create new user</a>



<ul>
  <% @userlistings.each do |userlisting| %>
    <li>
      <strong> <%= link_to(userlisting.title, userlisting_path(userlisting)) %>- <%= userlisting.location %> </strong>
    </li>
  <%end %>
</ul>
